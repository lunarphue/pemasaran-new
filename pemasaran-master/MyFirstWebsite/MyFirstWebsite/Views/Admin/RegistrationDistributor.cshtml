@model MyFirstWebsite.Models.Distributors
@{
    ViewBag.Title = "RegistrationDistributor";
}

<h2>RegistrationDistributor</h2>
@Html.ValidationSummary(true)

@using (Html.BeginForm())
{

    @Html.LabelFor(u => u.Kode_Distributor)<br />
    @Html.TextBoxFor(u => u.Kode_Distributor)<br />
    @Html.LabelFor(u => u.Name)<br />
    @Html.TextBoxFor(u => u.Name)<br />
    @Html.LabelFor(u => u.Alamat)<br />
    @Html.TextBoxFor(u => u.Alamat)<br />
    @Html.LabelFor(u => u.Kecamatan)<br />
    @Html.TextBoxFor(u => u.Kecamatan)<br />
    @Html.LabelFor(u => u.Telpon)<br />
    @Html.TextBoxFor(u => u.Telpon)<br />
    @Html.LabelFor(u => u.Faks)<br />
    @Html.TextBoxFor(u => u.Faks)<br />
    @Html.LabelFor(u => u.NPWP)<br />
    @Html.TextBoxFor(u => u.NPWP)<br />
    @Html.LabelFor(u => u.Aktif) <br />
    @Html.DropDownListFor(u => u.Aktif, new SelectList(
                  new List<Object>{
                       new { value = "A1" , text = "Aktif"  },
                       new { value = "A2" , text = "Non Aktif" }
                    }, "value", "text", 2))
    <button type="submit">Register</button>

}

